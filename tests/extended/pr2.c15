program pr2c15:
var x,y: integer;
begin
   for x := 0 to 11 do
      begin
	 case x of
	    1: output(x);
	    2..3: output(x);
	    4..5: for y := 1 to x do
		    output(x,y);
            6: ; 7: ;
	    8..9: begin
		     {do nothing}
		     end;
            10: output(999);
         otherwise
	    begin
	       output(888);
            end
         end;
      end;
end pr2c15.
