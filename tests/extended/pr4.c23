{ This program tests simple function calls for two arguments. }
program pr5c26:
var a, b, c, d: integer;

function pos (a, b:integer) : boolean;
var c: integer;
begin
	c := a;
	output(a, b, c, d);
	d := b + 1;
	if (a + b) > 0 then
		return(true)
	else
