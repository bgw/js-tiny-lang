program pr1c11:

    { this program tests the arithemetic operators }

    { the output should be:
         1000
         100
         200
         150
         3
	 81
    }

var
    two, result : integer;

begin

    two := 2;

    result := 1000;
    output(result);
    result := result / 10;
    output(result);
    result := result * two;
    output(result);
    result := result - 50;
    output(result);
    result := result mod 7;
    output(result);
    result := result ** 4;
    output(result);
    
end pr1c11.
