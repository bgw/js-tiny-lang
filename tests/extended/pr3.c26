program pr3c26:
{ This program checks the output of both integers and characters.

  OUTPUT SHOULD BE :
         Big is   1000
         Small is 10
         chr(65 ) = A
}
const
   big=1000;
   small=10;
var
   val:integer;

begin
   val:=55;
   output("Big is   ",big);
   output("Small is ",small);
   val := val + small;
   output("chr(",val,") = ",chr(val));
end pr3c26.
